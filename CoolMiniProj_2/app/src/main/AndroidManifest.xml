<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coolminiproj_2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/global_app_title"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Base.NoActionBar">
        <activity
            android:name=".SearchHistory"
            android:theme="@style/AppTheme.Base.NoActionBar" />
        <activity
            android:name=".MyLibraryActivity"
            android:theme="@style/AppTheme.Base.NoActionBar" />
        <activity android:name=".OneBookInfoActivity" />
        <activity android:name=".DisplaySearchResults" />
        <activity
            android:name=".MainActivity"
            android:label="@string/global_app_title"
            android:theme="@style/AppTheme.Base.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>